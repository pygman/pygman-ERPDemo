<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd
		">
	<bean id="roleAction" class="pygman.invoice.auth.role.web.RoleAction"
		scope="prototype">
		<property name="roleEbi" ref="roleEbi" />
		<property name="resEbi" ref="resEbi" />
		<property name="menuEbi" ref="menuEbi" />
	</bean>

	<bean id="roleEbi" class="pygman.invoice.auth.role.business.ebo.RoleEbo">
		<property name="roleDao" ref="roleDao" />
	</bean>

	<bean id="roleDao" class="pygman.invoice.auth.role.dao.impl.RoleImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
</beans>
